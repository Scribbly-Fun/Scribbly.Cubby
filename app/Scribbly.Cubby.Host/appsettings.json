{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Scribbly.Cubby.Server.Background": "Trace"
    }
  },
  "AllowedHosts": "*",
  "CubbyOptions": {
    "Store" : "Sharded",
    "Capacity": 1000,
    "Cleanup": {
      "Strategy": "Duration",
      "Delay" : "00:01:00"
    }
  }
}
