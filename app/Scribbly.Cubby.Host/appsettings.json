{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Scribbly.Cubby": "Information"
    }
  },
  "AllowedHosts": "*",
  "Cubby": {
    "Store" : "Sharded",
    "Cleanup": {
      "Strategy": "Duration",
      "Delay" : "00:00:01"
    }
  }
}
